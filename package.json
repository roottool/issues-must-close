{
  "name": "issues-must-close",
  "version": "0.1.0",
  "description": "We can find issues of open source projects.",
  "keywords": [
    "next",
    "nextjs",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/issues-must-close.git"
  },
  "bugs": {
    "url": "https://github.com/roottool/issues-must-close/issues"
  },
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format",
    "test": "echo \"Write tests!\" && exit 1",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "linaria": "^1.3.3",
    "next": "^9.5.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storysource": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/cli": "^6.0.26",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.26",
    "@types/react": "^16.9.53",
    "@types/react-helmet": "^6.1.0",
    "@types/react-select": "^3.0.22",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@zeit/next-css": "^1.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.5.0",
    "husky": ">=4.3.0",
    "lint-staged": ">=10.4.2",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "responsive-loader": "^2.2.0",
    "storybook-addon-designs": "^5.4.2",
    "typescript": "^4.0.3",
    "webp-loader": "^0.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js,jsx,ts,tsx)": "eslint --cache --fix",
    "*.(ts,tsx)": "tsc --noEmit",
    "*.{js,jsx,ts,tsx,md}": "prettier --write"
  }
}
